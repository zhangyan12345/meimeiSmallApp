.select-sex-box{
    position: fixed;
    top:100%;
    left:0px;
    width: 100%;
    height: 100%;
}
.select-sex-mask{
    position: absolute;
    top:0px;
    left:0px;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.3);
}
.select-wrapper{
    border-radius: 30rpx;
    background: #fff;
    width: 94%;
    margin-left: 3%;
    position: absolute;
    bottom:20rpx;
    left:0rpx;
}
.select-sex-item{
    line-height: 128rpx;
    height: 128rpx;
    border-bottom: 1rpx solid #a6a6a6;
    text-align:center;
    color: #000;
}
.select-sex-item:last-child{
    border:none;
}